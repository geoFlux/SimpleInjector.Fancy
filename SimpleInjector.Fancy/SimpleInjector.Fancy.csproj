<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Jason Blake</Authors>
    <Company>none</Company>
    <Description>For when you want to get a little bit fancy with simple injector.
Allows you to register by parameter name like so:

container.RegisterByParameterName&lt;string&gt;(localConnectionString =&gt; "my connection");</Description>
    <PackageProjectUrl>https://github.com/geoFlux/SimpleInjector.Fancy</PackageProjectUrl>
    <Version>1.1.0</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="SimpleInjector" Version="4.0.12" />
  </ItemGroup>

</Project>
